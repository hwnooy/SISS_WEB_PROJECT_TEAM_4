CREATE DATABASE EVERYTIME;
USE EVERYTIME;
CREATE TABLE review(
    id INT(11) NOT NULL AUTO_INCREMENT,
    수업명 VARCHAR(100) NOT NULL,
    교수명 VARCHAR(100) NOT NULL,
    과제 VARCHAR(100) NOT NULL,
    조모임 VARCHAR(100) NOT NULL,
    성적 VARCHAR(100) NOT NULL,
    출결 VARCHAR(100) NOT NULL,
    시험횟수 VARCHAR(100) NOT NULL,
    총점 INT(11) NOT NULL,
    수강학기 VARCHAR(100) NOT NULL,
    총평 TEXT NOT NULL,
    PRIMARY KEY(id));
